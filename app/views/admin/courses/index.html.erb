<div class="row mt-4">
  <%= render 'admin/search' %>
 <div class="col">
  <h1>受講者一覧</h1>
   <table class="table">
    <thead>
     <tr>
      <th scope="col">生徒名</th>
      <th scope="col">受講車種</th>
      <th scope="col">担当指導員</th>
      <th scope="col">在職状況</th>
      <th scope="col">入校日</th>
     </tr>
    </thead>
    <tbody>
      <% @courses.each do |course| %>
      <tr>
       <td><%= course.student.name %></td>
       <td><%= course.car_model %></td>
       <td><%= course.instructor.name %></td>
       <td><%= course.status %></td>
       <td><%= course.start_time %></td>
      </tr>
      <% end %>
    </tbody>
   </table>

 </div>
</div>